# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_native
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_native)
  gltfSaver_config = debug_native
endif
ifeq ($(config),debug_x64)
  gltfSaver_config = debug_x64
endif
ifeq ($(config),debug_x32)
  gltfSaver_config = debug_x32
endif
ifeq ($(config),release_native)
  gltfSaver_config = release_native
endif
ifeq ($(config),release_x64)
  gltfSaver_config = release_x64
endif
ifeq ($(config),release_x32)
  gltfSaver_config = release_x32
endif

PROJECTS := gltfSaver

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

gltfSaver:
ifneq (,$(gltfSaver_config))
	@echo "==== Building gltfSaver ($(gltfSaver_config)) ===="
	@${MAKE} --no-print-directory -C . -f gltfSaver.make config=$(gltfSaver_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f gltfSaver.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_native"
	@echo "  debug_x64"
	@echo "  debug_x32"
	@echo "  release_native"
	@echo "  release_x64"
	@echo "  release_x32"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   gltfSaver"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"